{
  "name": "agri-drone-ops",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "worker:dev": "tsx -r tsconfig-paths/register --env-file=.env workers/batch-worker.ts",
    "worker": "tsx -r tsconfig-paths/register workers/batch-worker.ts",
    "worker:watch": "tsx --watch --env-file=.env workers/batch-worker.ts",
    "worker:inference": "tsx -r tsconfig-paths/register workers/inference-worker.ts",
    "worker:inference:dev": "tsx -r tsconfig-paths/register --env-file=.env workers/inference-worker.ts",
    "worker:yolo-inference": "tsx -r tsconfig-paths/register workers/yolo-inference-worker.ts",
    "worker:yolo-inference:dev": "tsx -r tsconfig-paths/register --env-file=.env workers/yolo-inference-worker.ts",
    "seed:verified": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed-verified.ts",
    "docker:build": "docker build -t agridrone-ops .",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^3.943.0",
    "@aws-sdk/client-s3": "^3.857.0",
    "@aws-sdk/s3-request-presigner": "^3.857.0",
    "@mapbox/mapbox-gl-draw": "^1.5.0",
    "@mapbox/shp-write": "^0.4.3",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.83.0",
    "@types/bcryptjs": "^2.4.6",
    "@uppy/aws-s3": "^4.3.2",
    "@uppy/core": "^4.5.3",
    "@uppy/dashboard": "^4.4.3",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.65.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "exif-parser": "^0.1.12",
    "exifr": "^7.1.3",
    "fast-exif": "^2.0.1",
    "ioredis": "^5.8.2",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "mapbox-gl": "^3.13.0",
    "next": "15.4.2",
    "next-auth": "^4.24.11",
    "node-exif": "^1.0.1",
    "p-queue": "^9.0.1",
    "piexifjs": "^1.0.6",
    "prisma": "^6.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-leaflet": "^5.0.0",
    "react-leaflet-cluster": "^4.0.0",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.15",
    "tsconfig-paths": "4.2.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.57.0",
    "@types/leaflet": "^1.9.20",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
